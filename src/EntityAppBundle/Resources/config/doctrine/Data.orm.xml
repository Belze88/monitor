<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EntityAppBundle\Entity\Data" table="data">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datePrise" type="datetime" column="date_prise" nullable="false"/>
    <field name="deviceId" type="string" column="device_id" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="donneeBrute" type="string" column="donnee_brute" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="temperature" type="float" column="temperature" precision="10" scale="0" nullable="false"/>
    <field name="hygrometrie" type="integer" column="hygrometrie" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="pointeur" type="string" column="pointeur" length="4" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pointeurSuivant" type="string" column="pointeur_suivant" length="4" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="actif" type="boolean" column="actif" nullable="false"/>
    <field name="replicationId" type="bigint" column="replication_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="workField" type="string" column="work_field" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="offset" type="integer" column="offset" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
