<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EntityAppBundle\Entity\Commande" table="commande">
    <indexes>
      <index name="commande_id_data_idx" columns="data_id"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateDebut" type="datetime" column="date_debut" nullable="false"/>
    <field name="dateFin" type="datetime" column="date_fin" nullable="true"/>
    <field name="delta" type="float" column="delta" precision="10" scale="0" nullable="false"/>
    <field name="allumage" type="boolean" column="allumage" nullable="false"/>
    <field name="actif" type="boolean" column="actif" nullable="false"/>
    <field name="consigneElementId" type="bigint" column="consigne_element_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="alarme" type="boolean" column="alarme" nullable="false">
      <options>
        <option name="default">0</option>
      </options>
    </field>
    <field name="replicationId" type="bigint" column="replication_id" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="workField" type="string" column="work_field" length="64" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="data" target-entity="Data" fetch="LAZY">
      <join-columns>
        <join-column name="data_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
